package com.tsop.games.memory;

/// <summary>
/// structure to keep track of a pair of matching cards and which move we're on
/// </summary>
public class Match 
{
	public Position location1;
	public Position location2;
	public boolean firstMove;

	public Match(Position card1, Position card2, boolean move)
	{
		location1 = card1;
		location2 = card2;
		firstMove = move;
	}

}
